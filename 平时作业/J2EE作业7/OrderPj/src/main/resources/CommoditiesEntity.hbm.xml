<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="nju.edu.order.Model.CommoditiesEntity" table="Commodities" schema="J2EEOrderPj">
        <id name="commodityID" column="commodityID"/>
        <property name="commodityName" column="commodityName"/>
        <property name="commodityPrice" column="commodityPrice"/>
        <property name="url" column="url"/>
    </class>
</hibernate-mapping>