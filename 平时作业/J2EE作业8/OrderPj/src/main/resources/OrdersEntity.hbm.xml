<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="nju.edu.order.Model.OrdersEntity" table="Orders" schema="J2EEOrderPj">
        <id name="orderID" column="orderID"/>
        <property name="userID" column="userID"/>
        <property name="commodityID" column="commodityID"/>
        <property name="isValid" column="isValid"/>
        <property name="time" column="time"/>
        <property name="discountPrice" column="discountPrice"/>
        <property name="price" column="price"/>
        <property name="num" column="num"/>
    </class>
</hibernate-mapping>